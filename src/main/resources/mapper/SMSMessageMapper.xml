<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.User.Dao.SMSMessageMapper">

    <select id = "searchMessageContent" resultType="java.lang.String">
        select smsTemplate from SMS_TEMPLATE_TABLE where smsCodeType = #{smsCodeType}
    </select>
</mapper>